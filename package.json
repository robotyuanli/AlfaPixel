{
  "name": "AlfaPixel",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "env:local": "yarn clear:env && node scripts/set_env.js local",
    "env:local:ios": "yarn clear:env:ios && node scripts/set_env.js local",
    "env:production": "yarn clear:env && node scripts/set_env.js production",
    "env:production:ios": "yarn clear:env:ios && node scripts/set_env.js production",
    "env:staging": "yarn clear:env && node scripts/set_env.js staging",
    "env:staging:ios": "yarn clear:env:ios && node scripts/set_env.js staging",
    "env:dev": "yarn clear:env && node scripts/set_env.js dev",
    "env:dev:ios": "yarn clear:env:ios && node scripts/set_env.js dev",
    "clear:env": "del env.json",
    "clear:env:ios": "rm env.json",
    "android": "react-native run-android",
    "adev": "yarn adb-nox && yarn adb-reverse && yarn start",
    "adev:dev": "yarn adb-nox && yarn adb-reverse && yarn start:dev",
    "adev:staging": "yarn adb-nox && yarn adb-reverse && yarn start:staging",
    "adev:production": "yarn adb-nox && yarn adb-reverse && yarn start:production",
    "android-clean": "cd android && ./gradlew clean",
    "android-run-debug": "react-native run-android",
    "android-run-release": "yarn env:local && react-native run-android --variant=release",
    "android-run-release-dev": "yarn env:dev && react-native run-android --variant=release",
    "android-run-release-staging": "yarn env:staging && react-native run-android --variant=release",
    "android-run-release-production": "yarn env:production && react-native run-android --variant=release",
    "android-build-debug": "cd android/ && ./gradlew assembleDebug",
    "android-build-release": "cd android && ./gradlew assembleRelease",
    "android-build-release-dev": "yarn env:dev && cd android && ./gradlew assembleRelease",
    "android-build-release-staging": "yarn env:staging && cd android && ./gradlew assembleRelease",
    "android-build-release-production": "yarn env:production && cd android && ./gradlew assembleRelease",
    "android-signkey": "keytool -genkey -v -keystore release.keystore -alias rapidrecovery -keyalg RSA -keysize 2048 -validity 10000",
    "cr": "yarn android-clean && yarn android",
    "cr:dev": "yarn env:dev && yarn android-clean && yarn android",
    "cr:staging": "yarn env:staging && yarn android-clean && yarn android",
    "cr:production": "yarn env:production && yarn android-clean && yarn android",
    "adb-nox": "adb connect localhost:62025",
    "adb-reverse": "adb reverse tcp:8081 tcp:8081",
    "ios": "react-native run-ios --simulator=\"iPhone 11 Pro Max\"",
    "start": "react-native start",
    "start:dev": "yarn env:staging && react-native dev",
    "start:staging": "yarn env:staging && react-native start",
    "start:production": "yarn env:production && react-native start",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^5.9.6",
    "@react-native-community/segmented-control": "^2.1.1",
    "@react-navigation/bottom-tabs": "^5.8.0",
    "@react-navigation/drawer": "^5.9.0",
    "@react-navigation/native": "^5.7.3",
    "@react-navigation/stack": "^5.9.0",
    "axios": "^0.20.0",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.32",
    "react": "16.13.1",
    "react-native": "0.63.2",
    "react-native-animatable": "^1.3.3",
    "react-native-background-actions": "^2.2.0",
    "react-native-dropdown-picker": "^3.7.1",
    "react-native-fast-image": "^8.3.2",
    "react-native-flash-message": "^0.1.16",
    "react-native-geolocation-service": "^5.0.0",
    "react-native-gesture-handler": "^1.7.0",
    "react-native-google-places-autocomplete": "^2.1.2",
    "react-native-indicators": "^0.17.0",
    "react-native-loading-spinner-overlay": "^1.1.0",
    "react-native-maps": "^0.27.1",
    "react-native-modal": "^11.5.6",
    "react-native-paper": "^4.4.1",
    "react-native-reanimated": "^1.13.0",
    "react-native-safe-area-context": "^3.1.6",
    "react-native-safe-area-view": "^1.1.1",
    "react-native-screens": "^2.10.1",
    "react-native-splash-screen": "^3.2.0",
    "react-native-status-bar-height": "^2.5.0",
    "react-native-svg": "^12.1.0",
    "react-native-swipe-gestures": "^1.0.5",
    "react-native-vector-icons": "^7.0.0",
    "react-native-webview": "^11.3.1",
    "react-navigation-backhandler": "^2.0.1",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/runtime": "^7.11.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/react": "^17.0.3",
    "@types/react-native": "^0.64.2",
    "babel-jest": "^26.3.0",
    "eslint": "^7.7.0",
    "jest": "^26.4.2",
    "metro-react-native-babel-preset": "^0.63.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "16.13.1",
    "typescript": "^4.2.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
