{
  "name": "AlfaPixel",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android-clean": "cd android && ./gradlew clean",
    "android-run-debug": "react-native run-android",
    "android-run-release": "yarn env:local && react-native run-android --variant=release",
    "android-build-debug": "cd android/ && ./gradlew assembleDebug",
    "android-build-release": "cd android && ./gradlew assembleRelease",
    "android-signkey": "keytool -genkey -v -keystore release.keystore -alias alfapixel -keyalg RSA -keysize 2048 -validity 10000",
    "cr": "yarn android-clean && yarn android",
    "adb-nox": "adb connect localhost:62025",
    "adb-reverse": "adb reverse tcp:8081 tcp:8081",
    "ios": "react-native run-ios --simulator=\"iPhone 11 Pro Max\"",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ."
  },
